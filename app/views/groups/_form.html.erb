<%= form_for @group do |f| %>
<div class="form-group">
    <div class="form-group string optional">
        <%= f.label "title", :class => "string optional control-label" %>
        <%= f.text_field :title, :class => "string optonal form-control form-control" %>
    </div>

    <div class="form-group string optional">
        <%= f.label "descripton", :class => "string optional control-label" %>
    </div>

  </div>
  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submitting..."} %>
  <% end %>
  
